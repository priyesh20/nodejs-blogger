<%- include('../includes/head.ejs')%>
</head>
<body>
   <%- include('../includes/navigation.ejs')%>
   <main class="container">
      <% if(blogs.length > 0) { %>
         <% for (let blog of blogs) {%>
            <div class="blog">
               <h1 style="font-size: 4rem;"><%= blog.title%></h1>
               <hr style="background-color: #dda2b6;">
               <p style="font-size: 1.5rem;"><%= blog.description %></p>
               <a href="/admin/edit-blog/<%= blog._id%>?edit=true">Edit</a>
            </div>
         <% } %>
      <% } else { %>
         <h1>No Products Found!</h1>
      <% } %>
   </main>
<%- include('../includes/end.ejs') %>